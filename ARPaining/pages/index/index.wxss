/**index.wxss**/
.container{
  background: #fff
}
.img-list1{
    width: 100%;
    overflow: hidden;
}
.img-list1 .ul{
    margin-left:-2%;
}
.img-list1 .li{
    width:100%;
    height: 150px;
    overflow: hidden;
    position: relative;
    float:left;
    margin-left:2%;
    margin-top:10px;
}
.img-list1 .li navigator{
    width: 100%;
    height: 100%;
}
.img-list1 .li image{
    width: 100%;
    min-height: 100%;
}
.img-list1 .li text{
    position: absolute;
    bottom:0.2%;
    left: 0;
    z-index: 3;
    width: 96%;
    padding:0 2%;
    height: 30rpx;
    line-height: 20rpx;
    color:#fff;
}
.img-list1 .li .bg{
    position: absolute;
    bottom: 0;
    left: 0;
    z-index: 1;
    width: 100%;
    height: 36rpx;
    background-color: #000;
    opacity: .5;
}

.menu_container {
    display: flex;
    flex-wrap: wrap;
    width: 99%;
    height: 150rpx
}

.menu_item {
    width: 33%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 24rpx;
    box-sizing: border-box;
    padding-bottom: 10rpx;
    padding-top: 14rpx;
}
.menu_icon {
    width: 40px;
    height: 40px;
    padding-bottom: 5px;
}

